{
  "name": "muban",
  "version": "2.3.1",
  "description": "",
  "scripts": {
    "dev": "webpack-dev-server --config build-tools/config/webpack/webpack.config.js",
    "dev:code": "webpack --config build-tools/config/webpack/webpack.config.code.dev.js --watch",
    "build": "node ./build-tools/script/build",
    "preview": "http-server ./dist/site --p 9001 -s -o",
    "compile:code": "npm run build dist",
    "compile:partials": "npm run build partials",
    "compile:html": "npm run build html",
    "storybook": "webpack-dev-server --config build-tools/config/storybook/webpack.config.js",
    "storybook:build": "npm run build storybook",
    "storybook:preview": "http-server ./dist/storybook --p 9003 -s -o",
    "clean": "npm run build clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src --ext .js --cache",
    "lint:ts": "tslint src/**/*.ts -c .tslintrc.js -p tsconfig.json -t verbose",
    "lint:scss": "stylelint src/**/*.scss --cache",
    "analyze": "webpack-bundle-analyzer dist/bundlesize-profile.json ./dist/site",
    "prettify": "prettier --write \"src/**/*.{js,ts,scss,json}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "gitDir": "./",
    "linters": {
      "src/**/*.{js,ts,scss,json}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.js": [
        "npm run lint:js"
      ],
      "src/**/*.ts": [
        "npm run lint:ts"
      ],
      "src/**/*.scss": [
        "npm run lint:scss"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mediamonks/muban.git"
  },
  "author": "Arjan van Wijk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mediamonks/muban/issues"
  },
  "homepage": "https://github.com/mediamonks/muban#readme",
  "dependencies": {
    "@types/knockout": "^3.4.46",
    "babel-polyfill": "^6.23.0",
    "highlight.js": "^9.12.0",
    "jquery": "^3.2.1",
    "knockout": "^3.4.2",
    "lodash": "^4.17.4",
    "modernizr": "^3.3.1",
    "modernizr-loader": "^1.0.1",
    "muban-convert-hbs": "^1.0.0",
    "normalize.css": "^7.0.0",
    "qs": "^6.5.1",
    "seng-scss": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "svgo-loader": "^1.2.1"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.8",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-display-name": "^0.0.3",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-remove-debugger": "^6.8.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-preset-env": "^1.2.1",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "directory-named-webpack-plugin": "^2.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^4.0.2",
