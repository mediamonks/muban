{
  "name": "muban",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "webpack-dev-server --config build-tools/config/webpack/webpack.config.js",
    "prebuild": "npm-run-all clean",
    "build": "npm-run-all -p compile:partials compile:code -s compile:html",
    "compile": "webpack --config build-tools/config/webpack/webpack.config.js",
    "compile:partials": "webpack --config build-tools/config/webpack/webpack.config.partials.js",
    "compile:code": "webpack -p --config build-tools/config/webpack/webpack.config.dist.js",
    "compile:html": "node ./script/build.js",
    "clean": "shx rm -rf build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle-size": "webpack-bundle-analyzer bundlesize-profile.json build/",
    "prebundle-size": "webpack --profile --json --config build-tools/config/webpack/webpack.config.dist.js > bundlesize-profile.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mediamonks/muban.git"
  },
  "author": "Arjan van Wijk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mediamonks/muban/issues"
  },
  "homepage": "https://github.com/mediamonks/muban#readme",
  "dependencies": {
    "file-loader": "^0.11.2",
    "modernizr": "^3.3.1",
    "modernizr-loader": "^1.0.1",
    "normalize.css": "^5.0.0",
    "seng-scss": "^1.0.0",
    "url-loader": "^0.5.9"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.8",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-display-name": "^0.0.3",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-remove-debugger": "^6.8.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "directory-named-webpack-plugin": "^2.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "handlebars": "4.0.6",
    "handlebars-loader": "^1.4.0",
    "js-beautify": "^1.6.11",
    "loader-utils": "^1.0.2",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.3",
    "recursive-readdir": "^2.1.1",
    "sass-loader": "^6.0.2",
    "shx": "^0.2.2",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.17.0",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
